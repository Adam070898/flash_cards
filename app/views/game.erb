<div class="container">
  <h1>Question <%= params[:question_id].to_s%> <br><%=session[:lives]%> attempts left</h1>
  <div class="login_container">
    <p><%=@le_feedback%></p>
    <form action="/play/<%=session[:current]%>" method="post">
    <p>Question: <%=@question%></p>
    <p>Answer: <input name="answer" type="text" size="25"></p>
    <p><input type="submit"></p>
    </form>
  </div>
</div>